echo "Recompiling reslc to ensure it is up to date..."
RUSTFLAGS=-Awarnings cargo build --bin=reslc --release
echo "Warming up build scripts..."
RUSTFLAGS=-Awarnings cargo check --package=$1
echo "Compiling RESL artifacts..."
RUSTFLAGS=-Awarnings RUST_BACKTRACE=full RUSTC_WRAPPER=target/release/reslc cargo build --package=$1
